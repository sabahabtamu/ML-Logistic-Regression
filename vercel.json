{
  "version": 2,
  "functions": {
    "backend/main.py": {
      "runtime": "@vercel/python",
      "excludeFiles": "{frontend,node_modules,public,models/unused_files}/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "backend/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "frontend/$1"
    }
  ]
}
